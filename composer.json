{
    "name": "yiisoft/cache-memcached",
    "type": "library",
    "description": "Yii Caching Library - Memcached Handler",
    "keywords": [
        "yii",
        "framework",
        "cache",
        "memcached",
        "psr-16"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/cache-memcached/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/cache-memcached"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-memcached": "*",
        "psr/simple-cache": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cache\\Memcached\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cache\\Memcached\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    }
}
